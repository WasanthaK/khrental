{
  "name": "khrentals",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "overrides": {
    "path-to-regexp": "1.9.0",
    "router": {
      "path-to-regexp": "1.9.0"
    }
  },
  "scripts": {
    "setup-env": "node scripts/setup-env.js",
    "generate-env-config": "node scripts/generate-env-config.js",
    "dev": "npm run generate-env-config && node server.js",
    "dev:vite": "npm run generate-env-config && vite",
    "dev:simple": "npm run generate-env-config && vite --force",
    "dev:vite-only": "npm run generate-env-config && vite --host --port 5173",
    "build": "npm run generate-env-config && vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "migrate": "node src/db/runMigration.js",
    "add-properties-column": "node src/scripts/add_associated_properties_column.js",
    "execute-sql": "node src/db/executeSql.js",
    "find-deprecated": "node scripts/find_deprecated_tables.js",
    "fix-property-timestamps": "node src/scripts/fixPropertyTimestamps.js",
    "fix-app-users": "node src/scripts/fixAppUsersPropertyIds.js",
    "standardize-timestamps": "node src/scripts/standardizeTimestampColumns.js",
    "run-fixes": "node src/db/executeSql.js ../scripts/manual_fixes.sql",
    "bucket-test": "node src/scripts/bucketTest.js",
    "list-buckets": "node -e \"import('./src/services/bucketExplorer.js').then(({default: explorer}) => explorer.listAllBuckets().then(({buckets}) => console.log(buckets)))\"",
    "setup-storage": "node src/scripts/setup_storage.js",
    "setup-storage-policies": "node src/scripts/setup_storage_policies.js",
    "create-exec-sql": "node src/scripts/create_exec_sql_function.js",
    "setup-signature": "node src/scripts/setupAgreementSignature.js",
    "test-webhook": "node --experimental-modules scripts/test-local-webhook.js",
    "verify-azure": "node scripts/verify-azure-configuration.js",
    "proxy": "node cors-proxy.js",
    "dev:with-proxy": "concurrently \"npm run proxy\" \"npm run dev\"",
    "start": "node start-dev.js",
    "deploy:functions": "npx supabase functions deploy sendgrid-email",
    "test:mobile": "node mobile-test.js",
    "test:mobile:chrome": "node testing/mobile/chrome-test.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@floating-ui/react": "^0.27.5",
    "@heroicons/react": "^2.0.18",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^4.36.1",
    "@tiptap/extension-bullet-list": "^2.11.5",
    "@tiptap/extension-color": "^2.11.5",
    "@tiptap/extension-link": "^2.11.5",
    "@tiptap/extension-list-item": "^2.11.5",
    "@tiptap/extension-ordered-list": "^2.11.5",
    "@tiptap/extension-placeholder": "^2.11.5",
    "@tiptap/extension-table": "^2.11.7",
    "@tiptap/extension-table-cell": "^2.11.7",
    "@tiptap/extension-table-header": "^2.11.7",
    "@tiptap/extension-table-row": "^2.11.7",
    "@tiptap/extension-text-align": "^2.11.5",
    "@tiptap/extension-text-style": "^2.11.5",
    "@tiptap/extension-underline": "^2.11.5",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/react": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "axios": "^1.8.4",
    "body-parser": "^2.2.0",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "docx": "^9.3.0",
    "express": "4.18.2",
    "html-docx-js": "^0.3.1",
    "html-to-docx": "^1.8.0",
    "path-to-regexp": "1.9.0",
    "pdf-lib": "^1.17.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^4.12.0",
    "react-router-dom": "6.19.0",
    "react-toastify": "^9.1.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "cors-anywhere": "^0.4.4",
    "dotenv": "^16.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.31",
    "puppeteer": "^24.8.0",
    "supabase": "^2.20.12",
    "tailwindcss": "^3.3.5",
    "terser": "^5.39.0",
    "vite": "^6.3.4"
  }
}
